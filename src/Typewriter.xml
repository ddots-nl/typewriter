<?xml version="1.0" encoding="utf-8"?>
<widget id="ddots.typewriter.Typewriter"  pluginWidget="true" needsEntityContext="false" offlineCapable="true" supportedPlatform="Web" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Typewriter</name>
    <description>A simple yet powerful native javascript plugin for a cool typewriter effect.</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="General"> 
            <property key="renderMode" type="enumeration" defaultValue="text">
                <caption>Render mode</caption>
                <description>How to render the widget as a text element.</description>
                <enumerationValues>
                    <enumerationValue key="text">Text</enumerationValue>
                    <enumerationValue key="p">Paragraph</enumerationValue>
                    <enumerationValue key="h1">Heading 1</enumerationValue>
                    <enumerationValue key="h2">Heading 2</enumerationValue>
                    <enumerationValue key="h3">Heading 3</enumerationValue>
                    <enumerationValue key="h4">Heading 4</enumerationValue>
                    <enumerationValue key="h5">Heading 5</enumerationValue>
                    <enumerationValue key="h6">Heading 6</enumerationValue>
                </enumerationValues>
            </property>
            <property key="loop" type="boolean" defaultValue="true">
                <caption>Loop</caption>
                <description>Whether to keep looping or not.</description>
            </property>
            <property key="delay" type="integer" defaultValue="0">
                <caption>Type speed</caption>
                <description>The delay between each key when typing, 0 = natural.</description>
            </property>
            <property key="deleteSpeed" type="integer" defaultValue="0">
                <caption>Delete speed</caption>
                <description>The delay between deleting each character, 0 = natural.</description>
            </property>
            <property key="cursor" type="string" defaultValue="|">
                <caption>Cursor</caption>
                <description>String value to use as the cursor.</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Texts">
            <property key="textList" type="object" isList="true">
                <caption>List of texts</caption>
                <description/>
                <properties>
                    <propertyGroup caption="Text object">
                        <property key="text" type="textTemplate">
                            <caption>Text</caption>
                            <description />
                        </property>
                    </propertyGroup>
                </properties>
            </property>
        </propertyGroup>
    </properties>
</widget>